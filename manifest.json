{
  "manifest_version": 2,
  "name": "Music PlayList",
  "version": "0.1",
  "default_local": "en",
  "icons": {
    "128": "headphones.png"
  },
  "browser_action":{
    "default_icon":"headphones.png",
  "default_popup":"music.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.4.min.js", "js/content.js"]
    }
  ]
}
